{
  "presets": [
    ["env", {
      "targets": {
        "browsers": [
          "chrome 60",
          "chrome 59",
          "edge 15",
          "edge 14",
          "firefox 55",
          "firefox 54",
          "ie 11",
          "safari 10.1",
          "safari 10"
        ]
      }
    }],
    "react"
  ],
  "plugins": [
    "transform-object-rest-spread",
    "transform-object-assign",
    "transform-runtime"
  ]
}
